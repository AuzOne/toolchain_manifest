<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <remote  name="aosp"
           fetch="https://android.googlesource.com/toolchain/"
           review="https://android-review.googlesource.com/" />
  <remote  name="sm"
           fetch="https://github.com/SaberMod/" />
