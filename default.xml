  <project path="binutils/binutils-gnu" name="binutils-gdb" remote="gnu-bin-gdb" revision="binutils-2_25-branch" />
  <project path="build" name="toolchain_build" />
  <project path="build-scripts" name="sabermod-toolchain-build" />
  <project path="ccache" name="ccache" remote="aosp" revision="master" />
  <project path="clang" name="clang" remote="aosp" revision="master" />
  <project path="cloog/cloog-0.18.2" name="cloog-current" revision="0.18.2" />
  <project path="isl/isl-0.13" name="isl-current" revision="0.13" />
  <project path="expat" name="expat" remote="aosp" revision="master" />
  <project path="gcc/gcc-SaberMod" name="GCC_SaberMod" revision="4.8.4" />
  <project path="gcc/gcc-gnu-stable" name="gcc" remote="gcc-mirror" revision="gcc-4_8-branch" />
  <project path="gdb/gdb-android" name="gdb-saber" revision="7.8" />
  <project path="gdb/gdb-gnu" name="binutils-gdb" remote="gnu-bin-gdb" revision="gdb-7.8-branch" />  
  <project path="gmp/gmp-gnu" name="gnu-gmp" revision="6.0.0" />
